        <activity android:name=".QueryItem" >
        </activity>
        <activity android:name=".QueryResult" >
        </activity>
        <activity android:name=".Set" >
        </activity>
        <activity android:name=".UpdatePassword" >
        </activity>
        <provider
            android:name="com.example.mydaily.DatabaseProvider"
            android:authorities="com.example.mydaily.provider" >
        </provider>
        
        
        
        <provider
            android:name="com.example.mydaily.DatabaseProvideruser"
            android:authorities="com.example.mydaily.provider" >
